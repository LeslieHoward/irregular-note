{
  "name": "irregular-note",
  "version": "2.0.0",
  "description": "The contents of learning and collections, update irregularly",
  "homepage": "https://lesliehoward.github.io/irregular-note/notes",
  "license": "MIT",
  "scripts": {
    "build": "dumi build",
    "deploy": "gh-pages -d dist",
    "dev": "dumi dev",
    "prepare": "husky install && dumi setup",
    "start": "npm run dev"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "dumi": "^2.0.0-rc.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "authors": []
}
